    "gatsby-plugin-emotion": "^4.1.14",
    "gatsby-plugin-manifest": "^2.2.27",
    "gatsby-plugin-offline": "^3.0.19",
    "gatsby-plugin-react-helmet": "^3.1.14",
    "gatsby-plugin-sharp": "^4.25.1",
    "gatsby-plugin-tslint": "^0.0.2",
    "gatsby-plugin-typescript": "^2.1.16",
    "gatsby-source-filesystem": "^2.1.36",
    "gatsby-transformer-sharp": "^2.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "build": "gatsby build",
    "start": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "serve": "gatsby serve",
    "typecheck": "tsc --noEmit",
    "lint": "tslint --project .",
    "check": "yarn run lint && yarn run typecheck"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gloompi/gatsby-ts-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/gloompi/gatsby-ts-boilerplate/issues"
