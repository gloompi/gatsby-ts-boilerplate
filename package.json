{
  "name": "gatsby-ts-boilerplate",
  "private": false,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "author": "GloompiQue Web <gloompi@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
